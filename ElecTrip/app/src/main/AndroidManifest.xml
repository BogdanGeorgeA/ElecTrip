<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.navigation3">

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

    <application
        android:name=".Notificare"
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ProfileActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".ConfirmPassword"/>
        <activity android:name=".AccountSettingsActivity"/>
        <activity android:name=".ForgotPasswordFirstActivity"/>
        <activity android:name=".ForgotPasswordSecondActivity"/>
        <activity android:name=".ForgotPasswordThirdActivity"/>
        <activity android:name=".RegisterActivity"/>
        <activity android:name=".RegisterCarActivity"/>
        <activity android:name=".AddChargingPort"
            android:windowSoftInputMode="stateHidden|adjustResize|adjustPan"/>
        <activity android:name=".AddRoutingOptions"
            android:windowSoftInputMode="stateHidden|adjustResize|adjustPan" />
        <!--<activity android:name=".MainActivity" />-->
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <receiver android:name=".DirectReplyReceiver" />
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/map_key" />
    </application>
    <uses-feature
        android:glEsVersion="0x00020000"
        android:required="true" />

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <uses-permission android:name="android.permission.INTERNET" />



</manifest>